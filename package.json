    "@fortawesome/react-fontawesome": "^0.2.2",
    "firebase": "^12.2.1",
    "lint": "^0.8.19",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.35.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
