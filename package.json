    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.1.0"
  }
