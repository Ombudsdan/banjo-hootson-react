{
  "name": "banjo-hootson-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.config.cjs",
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production tsc -b && webpack --mode production --config webpack.config.cjs && npm run sitemap",
