{
  "name": "banjo-hootson-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.config.cjs",
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production npm run typecheck && webpack --mode production --config webpack.config.cjs && npm run sitemap",
    "build:dev": "cross-env NODE_ENV=development npm run typecheck && webpack --mode development --config webpack.config.cjs && npm run sitemap",
    "sitemap": "node ./scripts/generate-sitemap.cjs",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --reporter=default",
    "test:watch": "vitest",
    "docker:start": "docker run --rm -p 8080:8080 ombudsdan/banjo-hootson-react:dev",
    "docker:stop": "docker rm -f banjo-hootson-react",
    "commit": "cross-env CZ_CONFIG=.cz-config.cjs git-cz",
    "prepare": "husky",
    "verify-lock": "npm ci --dry-run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "firebase": "^12.4.0",
    "googleapis": "^164.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.38.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitejs/plugin-react": "^5.1.0",
    "babel-loader": "^10.0.0",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.5.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "react-refresh": "^0.18.0",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.6",
    "semantic-release": "^25.0.1",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.4",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "overrides": {
    "conventional-changelog-writer": "^8.0.0"
  }
}
